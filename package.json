{
  "name": "sveltekit-authentication-cookies",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "vercel-postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "vercel-build": "prisma generate && pnpm build && pnpm vercel-postbuild"
  },
  "devDependencies": {
    "@playwright/test": "1.23.1",
    "@prisma/client": "4.0.0",
    "@sveltejs/adapter-vercel": "1.0.0-next.59",
    "@sveltejs/kit": "v1.0.0-next.357",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie": "0.5.1",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "bcryptjs": "2.4.3",
    "cookie": "0.5.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "prisma": "4.0.0",
    "svelte": "3.48.0",
    "svelte-check": "2.8.0",
    "svelte-preprocess": "4.10.7",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "type": "module"
}
